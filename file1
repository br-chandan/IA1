pipeline {
    agent any
    stages {
        stage('BranchName') {
            steps {
                script {
                    def branchName = env.BRANCH_NAME
        
                    if (branchName == 'main') {
                        echo 'This is a main branch.'
                    } else {
                        echo "This is a dev branch ."
                    }
                }
            }
        }
    }
}
